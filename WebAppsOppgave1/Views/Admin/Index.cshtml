    function ValidateFlight(regOrEd, fromAirport, toAirport, departure, arrival, price) {
        HideFlightWarningLabels();
        moment.locale("nb");
        var momDeparture = moment(departure, "DD.MM.YYYY HH:mm");
        var momArrival = moment(arrival, "DD.MM.YYYY HH:mm");
        var re;
        var valid = true;

        if (fromAirport === "") {
            $("#fromAirport" + regOrEd + "Lbl").html("Velg en flyplass å reise fra");
            $("#fromAirport" + regOrEd + "Lbl").show();
            valid = false;
        }
        if (toAirport === "") {
            $("#toAirport" + regOrEd + "Lbl").html("Velg en flyplass å reise til");
            $("#toAirport" + regOrEd + "Lbl").show();
            valid = false;
        }
        re = new RegExp("^[0-9]{2}.[0-9]{2}.[0-9]{4} [0-9]{2}:[0-9]{2}$");
        if (!momDeparture.isValid()) {
            $("#departure" + regOrEd + "Lbl").html("Må skrives som dd.mm.yyyy hh:mm");
            $("#departure" + regOrEd + "Lbl").show();
            valid = false;
        }
        else if (!re.test(departure)) {
            $("#departure" + regOrEd + "Lbl").html("Må skrives som dd.mm.yyyy hh:mm");
            $("#departure" + regOrEd + "Lbl").show();
            valid = false;
        }
        else if (momDeparture < moment()) {
            $("#departure" + regOrEd + "Lbl").html("Avreise satt i fortiden");
            $("#departure" + regOrEd + "Lbl").show();
            valid = false;
        }
        if (!momArrival.isValid()) {
            $("#arrival" + regOrEd + "Lbl").html("Må skrives som dd.mm.yyyy hh:mm");
            $("#arrival" + regOrEd + "Lbl").show();
            valid = false;
        }
        else if (!re.test(arrival)) {
            $("#arrival" + regOrEd + "Lbl").html("Må skrives som dd.mm.yyyy hh:mm");
            $("#arrival" + regOrEd + "Lbl").show();
            valid = false;
        }
        else if (momArrival <= momDeparture) {
            $("#arrival" + regOrEd + "Lbl").html("Ankomst må være etter avreise");
            $("#arrival" + regOrEd + "Lbl").show();
            valid = false;
        }
        if (!parseInt(price)) {
            $("#price" + regOrEd + "Lbl").html("Pris må være et heltall");
            $("#price" + regOrEd + "Lbl").show();
            valid = false;
        }
        else if (parseInt(price) < 0 || parseInt(price) > 20000) {
            $("#price" + regOrEd + "Lbl").html("Pris må være mellom kr 0 og 20 000");
            $("#price" + regOrEd + "Lbl").show();
            valid = false;
        }
        return valid;
    }

